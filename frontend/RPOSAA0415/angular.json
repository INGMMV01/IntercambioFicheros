{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "analytics": false,
    "defaultCollection": "@morphe/schematic-angular"
  },
  "newProjectRoot": "projects",
  "projects": {
    "RPOSAA0415": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "abanca",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/RPOSAA0415",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/assets/applicationManifest.json",
              {
                "glob": "web.config.*",
                "input": "configWebServer",
                "output": "/"
              }
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": [],
            "allowedCommonJsDependencies": [
              "crypto-js",
              "semver",
              "mime"
            ],
            "localize": true,
            "i18nMissingTranslation": "error"
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "extractLicenses": true,
              "optimization": true,
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "buildOptimizer": true,
              "subresourceIntegrity": false,
              "progress": false,
              "verbose": true,
              "crossOrigin": "anonymous"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "aot": true,
              "progress": false,
              "verbose": true,
              "crossOrigin": "anonymous"
            },
            "es5": {
              "tsConfig": "./tsconfig-es5.app.json"
            },
            "es-ES": {
              "localize": [
                "es-ES"
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "RPOSAA0415:build:production"
            },
            "development": {
              "browserTarget": "RPOSAA0415:build:development,es-ES"
            },
            "es5": {
              "browserTarget": "RPOSAA0415:build:es5"
            },
            "es-ES": {
              "browserTarget": "RPOSAA0415:build:es-ES"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "hmr": true
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "RPOSAA0415:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/**/*.ts",
              "src/**/*.html"
            ]
          }
        }
      },
      "i18n": {
        "sourceLocale": {
          "code": "es-ES",
          "baseHref": ""
        },
        "locales": {}
      }
    }
  },
  "defaultProject": "RPOSAA0415"
}